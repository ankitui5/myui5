<core:FragmentDefinition xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout">

	<Dialog title="Upload Yube Defect Photos" contentWidth="30%" contentHeight="20%" verticalScrolling="true" class="abcsapUiSizeCompact">
		<content>
		
		<l:Grid>
			<l:content>
				<VBox alignItems="Center">
					<Text id="idTubeMD" text="Major Defect" ></Text>
					<HBox alignItems="Center">
					<!-- <Button icon="sap-icon://add-photo" id="idbtn1"></Button> -->
					<core:Icon id="idiconTubeMD" src="sap-icon://add-photo" press="onTubeMajorDefect" size="4rem"/>
					</HBox>
						<layoutData>
							<l:GridData span="L4 M3 S12" />
						</layoutData>
				</VBox>
				
				<VBox alignItems="Center">
					<Text id="idTubeODV" text="Other Defect/View"></Text>
					<HBox alignItems="Center">
					<!-- <Button width="" icon="sap-icon://add-photo" id="idbtn2"></Button> -->
					<core:Icon id="idiconTubeODV" src="sap-icon://add-photo" press="onOtherDefectView" size="4rem"/>
					</HBox>
						<layoutData>
							<l:GridData span="L4 M3 S12" />
						</layoutData>
				</VBox>
				
				
			</l:content>
		</l:Grid>
		
			
			<!--  	 -->
 
 
		</content>

		<beginButton>
			<Button text="OK" press="OnDefectTubePhotosOk" /> 
		</beginButton>
		<!-- <endButton>
			<Button text="Cancel" press="OnDefectPhotoscancel" />
		</endButton> -->
	</Dialog>
</core:FragmentDefinition>		
		