<core:FragmentDefinition xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" 
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout">



<Dialog title="Upload Tube Photos" contentWidth="30%" contentHeight="20%" verticalScrolling="true" class="abcsapUiSizeCompact">
	<content>
		<l:Grid>
			<l:content>
				<VBox alignItems="Center">
					<Text id="idTubeSizMarking" text="Size Marking" ></Text>
					<HBox alignItems="Center">
					<!-- <Button icon="sap-icon://add-photo" id="idbtn1"></Button> -->
					<core:Icon id="idicon1" src="sap-icon://add-photo" press="onTubeSizeMarking" size="4rem"/>
					</HBox>
						<layoutData>
							<l:GridData span="L4 M3 S12" />
						</layoutData>
				</VBox>
				
				<VBox alignItems="Center">
					<Text id="idTubeMouldMMYY" text="Mould+MMYY"></Text>
					<HBox alignItems="Center">
					<!-- <Button width="" icon="sap-icon://add-photo" id="idbtn2"></Button> -->
					<core:Icon id="idicon2" src="sap-icon://add-photo" press="onTubeMouldMMYY" size="4rem"/>
					</HBox>
						<layoutData>
							<l:GridData span="L4 M3 S12" />
						</layoutData>
				</VBox>
				
			</l:content>
		</l:Grid>
		
 
		</content>

		<beginButton>
			<Button text="OK" press="OnTubePhotoOk" /> 
		</beginButton>
		<!-- <endButton>
			<Button text="Cancel" press="OnTyrePhotocancel" />
		</endButton> -->
	</Dialog>
</core:FragmentDefinition>		
		