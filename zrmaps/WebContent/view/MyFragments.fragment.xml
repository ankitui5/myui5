<core:FragmentDefinition 
		xmlns="sap.m"
		xmlns:f="sap.ui.layout.form" 
		xmlns:core="sap.ui.core" 
		xmlns:l="sap.ui.layout">
		
<Dialog xmlns="sap.m" title="My Fragments"  contentWidth="80%" contentHeight="70%" class="abcsapUiSizeCompact">
<content>
<Panel id="Panel1" class="Custom_Frag">
<Table id="idTable" items="" visible="true" showNoData="false" fixedLayout="false" alternateRowColors="true" class="clsfrag"> 

		<headerToolbar>
			<Toolbar class="toolbarfrag">
			<ToolbarSpacer />
				<!-- <Text text="Reports" /> -->
				<Text id="idEmpName" text=""  />
			<ToolbarSpacer />
			    <Text id="idEmpId" text=""  />
			<ToolbarSpacer />
									
			</Toolbar>
		</headerToolbar>
				<columns>
					<Column width="2rem" minScreenWidth="Tablet" hAlign='Center' demandPopin="true">
						<Label text="S.No" design="Bold"/>
					</Column>
								
					<Column width="20rem" minScreenWidth="Tablet" hAlign='Center' demandPopin="true">
						<Label text="Check Points" design="Bold"/>
					</Column>
					
					<Column width="8rem"  minScreenWidth="Tablet"  hAlign='Center' demandPopin="true">
						<Label text="Answer" design="Bold"/>
					</Column>
										
					<Column width="15rem" minScreenWidth="Tablet" hAlign='Center' demandPopin="true">
						<Label text="Comment" design="Bold"/>
					</Column>	
				</columns>
				
				<items>
				
					<ColumnListItem>
						<cells>
							 <Text text=""/>
							 <Text text="" />
							 <Select selectedKey="" items="" forceSelection="false">
							 </Select>
							 <Textarea value="" rows="1" Cols="6" />
						</cells> 
				   </ColumnListItem> 
			   </items>
			    
	 </Table>  
</Panel>	 
</content> 

<beginButton>
	<Button text="Submit" id="idsubmit1" press="onCloseForm" />
</beginButton>
<endButton>
	<Button text="Cancel" press="Oncancel1" /> 
</endButton>
</Dialog>
</core:FragmentDefinition>

